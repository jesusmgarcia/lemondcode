{
	"name": "webpack-first",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "run-p -l type-check:watch start:dev",
		"start:dev": "webpack serve --config ./config/webpack.dev.js",
		"start:prod": "webpack serve --config ./config/webpack.prod.js",
		"build:dev": "npm run type-check && webpack --config ./config/webpack.dev.js",
		"build:prod": "npm run type-check && webpack --config ./config/webpack.prod.js",
		"build:perf": "npm run type-check && webpack --config ./config/webpack.perf.js",
    "type-check": "tsc --noEmit --project ./config",
		"type-check:watch": "npm run type-check -- --watch"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.13.14",
		"@babel/core": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "*",
		"css-loader": "^5.2.2",
		"dotenv-webpack": "^7.0.2",
		"html-loader": "^2.1.2",
		"html-webpack-plugin": "^5.3.1",
		"mini-css-extract-plugin": "^1.4.1",
		"npm-run-all": "^4.1.5",
		"sass": "^1.32.10",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"typescript": "^4.2.4",
		"webpack": "^5.33.2",
		"webpack-bundle-analyzer": "^4.4.1",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.7.3"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
